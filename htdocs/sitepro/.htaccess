RewriteEngine On
RewriteBase /

# Excepci√≥n para archivos .html (permite acceso directo a archivos .html)
RewriteCond %{REQUEST_URI} !\.html$ [NC]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^.*$ index.php [L,QSA]

